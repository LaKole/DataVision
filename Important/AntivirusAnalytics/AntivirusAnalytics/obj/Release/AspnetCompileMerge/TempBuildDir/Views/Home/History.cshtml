@model IEnumerable<AntivirusAnalytics.Models.History>


<table class="table">
    <tr>
        <th>
            Created
        </th>
        <th>
            Chart Type
        </th>
        <th>
            Options
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChartTypeProper)
            </td>
            <td style="display:none;">@Html.DisplayFor(modelItem => item.ID)</td>
            <td>
                <a href="javascript:;" onclick="recreateImage(this)">REGENERATE</a> |
                <a href="javascript:;" onclick="deleteImage(this)">DELETE</a> 
            </td>
            <td style="display:none;">@Html.DisplayFor(modelItem => item.ChartType)</td>
        </tr>
    }

</table>




